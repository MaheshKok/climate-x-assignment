{
  "name": "asset-management-system",
  "version": "1.0.0",
  "description": "Asset management system with file upload and display",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --quiet",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:check": "eslint . --ext .js,.jsx,.ts,.tsx --format table",
    "type-check": "tsc --noEmit --project tsconfig.json --skipLibCheck && tsc --noEmit --project tsconfig.test.json --skipLibCheck",
    "type-check:watch": "tsc --noEmit --watch",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "check": "npm run type-check && npm run lint && npm run format:check && npm run test:ci",
    "fix": "npm run lint:fix && npm run format",
    "audit": "npm audit --audit-level moderate",
    "complexity": "eslint . --ext .js,.jsx,.ts,.tsx --format json | jq '.[] | select(.messages[].ruleId == \"complexity\") | {file: .filePath, complexity: .messages[].message}'",
    "build:check": "npm run check && npm run build",
    "pre-commit": "lint-staged",
    "prepare": "husky install"
  },
  "keywords": [
    "nextjs",
    "asset-management",
    "file-upload",
    "chakra-ui"
  ],
  "author": "Climate-X Assignment",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@chakra-ui/icons": "^2.2.4",
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@tanstack/react-query": "^5.87.4",
    "@tanstack/react-query-devtools": "^5.87.4",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.14.0",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "csv-parser": "^3.2.0",
    "framer-motion": "^6.5.1",
    "multer": "^2.0.2",
    "next": "^14.2.0",
    "papaparse": "^5.5.3",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^15.5.3",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.5.3",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^0.25.1",
    "eslint-plugin-unicorn": "^55.0.0",
    "eslint-plugin-unused-imports": "^4.2.0",
    "husky": "^8.0.0",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "lint-staged": "^16.1.6",
    "node-mocks-http": "^1.17.2",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.1"
  }
}
